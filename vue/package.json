{
  "name": "vue-cookbook",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "yarn lint:ci --fix",
    "format": "prettier \"{src,bin,scripts}/**/*.{ts,tsx,js,jsx,json,gql,scss,css,vue}\" \"./.?*.{md,js,yaml,json}\"",
    "lint:ci": "eslint \"src/**/*.{js,ts,tsx,vue}\" --format=stylish",
    "format:ci": "yarn format --list-different"
  },
  "dependencies": {
    "@headlessui/vue": "1.7.14",
    "@heroicons/vue": "2.0.18",
    "@tanstack/vue-query": "4.32.0",
    "@tanstack/vue-table": "8.9.3",
    "clsx": "2.0.0",
    "date-fns": "2.30.0",
    "i18next": "23.4.1",
    "i18next-browser-languagedetector": "7.1.0",
    "i18next-http-backend": "2.2.1",
    "i18next-vue": "2.2.1",
    "pinia": "2.1.4",
    "pinia-plugin-persistedstate": "3.2.0",
    "tailwind-merge": "1.14.0",
    "vee-validate": "4.11.1",
    "vue": "^3.3.4",
    "vue-router": "4.2.4",
    "vue3-popper": "1.5.0",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.3.2",
    "@tailwindcss/forms": "0.5.4",
    "@tsconfig/node18": "18.2.0",
    "@types/node": "18.16.17",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/eslint-config-typescript": "11.0.3",
    "@vue/tsconfig": "0.4.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.39.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-vue": "9.15.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.26",
    "prettier": "3.0.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vue-tsc": "1.8.6"
  }
}
